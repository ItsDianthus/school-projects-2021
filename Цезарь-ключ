str1 = str(input())
str2 = str(input())
alf = 'abcdefghijklmnopqrstuvwxyz'
cnt = 0
num_old = 0
for i in range(len(str2)):
    index2 = alf.find(str2[i])
    index1 = alf.find(str1[i])
    num = index2 - index1
    if num < 0:
        num = 26 + num
    if num != num_old:
        cnt += 1
        num_old = num
if cnt > 1:
    print('Error')
else:
    print(num)
